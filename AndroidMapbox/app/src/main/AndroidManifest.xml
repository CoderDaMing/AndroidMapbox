<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ming.androidmapbox">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MapActivity"/>
        <activity android:name=".marker.CustomMarkerOneActivity"/>
        <activity android:name=".marker.CustomMarkerTwoActivity"/>
        <activity android:name=".marker.CustomMarkerThreeActivity"/>
        <activity android:name=".marker.InfoWindowAdapterActivity"/>
        <activity android:name=".symbol.GeoActivity"/>
        <activity android:name=".symbol.ReverseGeoActivity"/>
        <activity android:name=".symbol.SymbolLayerOneActivity"/>
        <activity android:name=".symbol.SymbolLayerTwoActivity"/>
        <activity android:name=".symbol.ClusterActivity"/>
        <activity android:name=".symbol.InfoWindowActivity"/>
        <activity android:name=".symbol.SnapshotActivity"/>
        <activity android:name=".symbol.HideMapNameActivity"/>
        <activity android:name=".symbol.LocationComponentActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ming.androidmapbox.root.StyleActivity" />
        <activity android:name="com.ming.androidmapbox.root.UiSettingActivity" />
        <activity android:name="com.ming.androidmapbox.root.CameraPositionActivity" />
        <activity android:name="com.ming.androidmapbox.root.MapListenerActivity" />

        <activity android:name="com.ming.androidmapbox.layers.FillLayerActivity" />
        <activity android:name="com.ming.androidmapbox.layers.LineLayerActivity" />
        <activity android:name="com.ming.androidmapbox.layers.SymbolLayerActivity" />
        <activity android:name="com.ming.androidmapbox.layers.SymbolLayerIconsActivity" />
        <activity android:name="com.ming.androidmapbox.layers.MarkerFollowingRouteActivity"/>
        <activity android:name="com.ming.androidmapbox.layers.CircleLayerActivity"/>
        <activity android:name="com.ming.androidmapbox.layers.CircleToIconTransitionActivity"/>
        <activity android:name="com.ming.androidmapbox.layers.CircleLayerClusteringActivity"/>
        <activity android:name="com.ming.androidmapbox.layers.CircleCreateHotspotsActivity"/>
    </application>
</manifest>